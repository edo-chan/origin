{
  "name": "template-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "perf:bundle-size": "npm run build && npx bundlesize",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build --output-dir dist-storybook",
    "clean": "rm -rf .next && rm -rf dist-storybook && rm -rf tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@vanilla-extract/css": "^1.16.1",
    "@vanilla-extract/next-plugin": "^2.4.7",
    "@vanilla-extract/sprinkles": "^1.6.3",
    "jotai": "^2.12.1",
    "next": "^15.3.0",
    "protobufjs": "^7.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vanilla-extract/webpack-plugin": "^2.3.22",
    "autoprefixer": "^10.4.21",
    "bundlesize": "^0.18.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.3.0",
    "eslint-plugin-storybook": "^0.11.2",
    "lighthouse": "^12.3.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "storybook": "^8.6.14",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "ts-proto": "^2.5.0",
    "typescript": "^5.7.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "bundlesize": [
    {
      "path": ".next/static/js/pages/_app-*.js",
      "maxSize": "250kb"
    },
    {
      "path": ".next/static/js/pages/index-*.js",
      "maxSize": "200kb"
    },
    {
      "path": ".next/static/js/chunks/framework-*.js",
      "maxSize": "200kb"
    },
    {
      "path": ".next/static/js/chunks/vendor-*.js",
      "maxSize": "300kb"
    },
    {
      "path": ".next/static/js/chunks/components-*.js",
      "maxSize": "150kb"
    }
  ]
}
